{
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true
    },
    "rules": {
        "import/no-unresolved": [2]
    },
    "globals": {
        "Phaser": true
    },
    "settings": {
        "import/core-modules": [ "phaser" ]
    }
}